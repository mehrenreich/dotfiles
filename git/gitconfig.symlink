[alias]
    br = branch
    ci = commit
    co = checkout
    diff-previous = !git diff HEAD^ HEAD
    dp = diff-previous
    st = status
    sw = switch

[commit]
    gpgsign = true

[gpg]
    program = gpg

[init]
    defaultBranch = main

